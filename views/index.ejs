    function isValidEmail(email) {
      // Basic email validation using regex
      const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
      return emailRegex.test(email);
    }
    // this function for the email must contain @ ........................................................................
    document.getElementById("txtEmail").addEventListener("input", function () {
      var emailInput = document.getElementById("txtEmail").value;
      var emailError = document.getElementById("emailError");
      var atIndex = emailInput.indexOf("@");
      if (atIndex === -1) {
        emailError.textContent = "Email must contain @ symbol";
      } else {
