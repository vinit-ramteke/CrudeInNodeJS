    // ........................address 1......................
    function populateCities1() {
      var stateSelect = document.getElementById("txtselectState");
      var citySelect = document.getElementById("txtCity_or_Village");
      var selectedState = stateSelect.value;

      // Clear existing options
      citySelect.innerHTML = "<option value='0'>Select City</option>";

      // Add options based on selected state
      if (selectedState !== "0") {
        citiesByState[selectedState].forEach(function (city) {
          var option = document.createElement("option");
          option.text = city;
          option.value = city;
          citySelect.add(option);
        });
      }
    }
    document.getElementById("txtselectState").addEventListener("change", populateCities1);
    populateCities();

